record(stringin, "BL6:SE:Rheometer:StrReadRheoVersion") {
  field(DESC, "Read Data sent from Hardware rheoversion")
  field(DTYP, "stream")
  field(INP, "@spyTxFromHardware.proto ReadRheoVersion RxFromHrdwr")
  field(SCAN, "I/O Intr")
  info(archive, "Monitor, 00:01:00")
}




